<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.hasys.cropper.sample">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.github.hasys.cropper.sample.LauncherActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.github.hasys.cropper.sample.SampleActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.github.hasys.cropper.sample.ResultActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".CropperActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".WeiChatCropActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.github.hasys.cropper.sample.WeiChatActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>
